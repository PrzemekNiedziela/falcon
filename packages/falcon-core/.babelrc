{
  "presets": [
    ["env", {
      "targets": { "node": 8 },
      "useBuiltIns": true,
      "modules": false
    }],
    "stage-0",
    "flow"
  ],
  "env": {
    "development": {
      "plugins": [
        "transform-class-properties",
        "transform-es2015-classes",
        ["flow-runtime", {
          "assert": true,
          "annotate": true
        }]
      ]
    }
  }
}
